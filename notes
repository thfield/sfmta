rails generate scaffold Stop stop_name:string stop_lat:decimal stop_lon:decimal
rails generate scaffold Trip route_id:integer service_id:integer trip_headsign:string direction_id:integer block_id:integer shape_id:integer
rails generate scaffold Fare price:decimal currency_type:string payment_method:integer transfers:integer transfer_duration:integer
rails generate scaffold Fare_route fare_id:integer route_id:integer
